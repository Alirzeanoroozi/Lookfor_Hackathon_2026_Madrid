{
    "arrowParens": "always",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "semi": false,
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 100,
    "endOfLine": "lf",
    "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-packagejson", "prettier-plugin-tailwindcss"],
    "importOrder": [
        "^(react/(.*)$)|^(react$)",
        "^(next/(.*)$)|^(next$)",
        "<THIRD_PARTY_MODULES>",
        "",
        "^@/types/(.*)$",
        "^@/components/ui/(.*)$",
        "^@/components/(.*)$",
        "^@/(.*)$",
        "",
        "^[./]"
    ]
}